<div class="chart-labels row">
	<div class="col">
		<p class="label-time">3 Years</p>
	</div>
	<div class="col">
		<p class="label">
			<img
				src="data:image/svg+xml;base64," />
			Avg. slope rate<span> -2.5 cm</span>
		</p>
	</div>
	<div class="col">
		<p class="label">
			<img
				src="data:image/svg+xml;base64," />
			Avg. slope rate<span> -2.5 cm</span>
		</p>
	</div>
	<div class="clear"></div>
</div>
<div class="chart" style="width: 500px; height: 120px; " id="<%= chart_id %>"></div>
<script>
	function myFunction(id) {
		var dom = document.getElementById(id);
		var myChart = echarts.init(dom);
		var app = {};
		option = null;

		option = {
			responsive: true,
			title: {
				show: false
			},
			tooltip: {
				show: false

			},
			legend: {
				show: false,
				data: ['邮件营销', '联盟广告', '视频广告', '直接访问', '搜索引擎'],
			},
			grid: {
				left: '0%',
				right: '2%',
				bottom: '5%',
				top: '5%',
				containLabel: true
			},
			xAxis: {
				type: 'category',
				boundaryGap: false,
				data: ['周一', '周二', '周三', '周四', '周五', '周六', '周日'],
				splitLine: {
					show: true,
				},
			},
			yAxis: {
				type: 'value'
			},
			series: [
				{
					name: '邮件营销',
					type: 'line',
					stack: '总量',
					symbol: 'none',
					data: [120, 132, 101, 134, 90, 230, 210],
					animation: false
				},
				{
					name: '联盟广告',
					type: 'scatter',
					stack: '总量',

					data: [220, 182, 191, 234, 290, 330, 310],
					animation: false
				},

			]
		};

		if (option && typeof option === 'object') {
			myChart.setOption(option, true);
		}
	}
	myFunction('<%= chart_id %>');
</script>